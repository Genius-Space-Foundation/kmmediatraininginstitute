{
  "name": "kmmedia-course-registration",
  "version": "1.0.0",
  "description": "Secure course registration system with JWT authentication",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install"
  },
  "keywords": [
    "react",
    "nodejs",
    "jwt",
    "authentication",
    "course-registration"
  ],
  "author": "KM Media",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "react-icons": "^5.5.0",
    "react-responsive-carousel": "^3.2.23",
    "typescript": "^5.9.2"
  }
}
